{
    "name": "alexasomba/paystack",
    "version": "1.1.0",
    "description": "Paystack API client for PHP",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://github.com/alexasomba/paystack-php",
    "license": "MIT",
    "authors": [
        {
            "name": "alexasomba"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Alexasomba\\\\Paystack\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alexasomba\\\\Paystack\\Test\\": "test/"
        }
    },
    "support": {
        "issues": "https://github.com/alexasomba/paystack-php/issues",
        "source": "https://github.com/alexasomba/paystack-php"
    }
}